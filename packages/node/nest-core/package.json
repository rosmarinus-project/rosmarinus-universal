{
  "name": "@rosmarinus/nest-core",
  "version": "2.1.6",
  "description": "nest core modules for rosmarinus projects",
  "main": "dist/cjs/index.cjs",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "dev": "rollup -c rollup.config.js -w",
    "build": "rimraf dist && rollup -c rollup.config.js",
    "test:unit": "jest --runInBand"
  },
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/es/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "directory": "packages/node/nest-core",
    "url": "https://github.com/rosmarinus-project/rosmarinus-universal.git"
  },
  "keywords": [
    "nest",
    "rosmarinus"
  ],
  "license": "MIT",
  "dependencies": {
    "@rosmarinus/common-utils": "workspace:^3.1.0",
    "@rosmarinus/node-utils": "workspace:^0.3.0",
    "ioredis": "^5.4.1",
    "knex": "^3.1.0",
    "mysql2": "^3.10.1",
    "tslib": "^2.6.2"
  },
  "peerDependencies": {
    "@nestjs/common": ">=10.3.3",
    "express": ">=4.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-proposal-decorators": "^7.23.9",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/plugin-transform-private-methods": "^7.23.3",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@jest/globals": "^29.7.0",
    "@nestjs/common": "^10.3.3",
    "@rosmarinus/common-plugins": "workspace:*",
    "@rosmarinus/eslint-config-rosmarinus": "^1.0.4",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.19",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "jest-json-reporter": "^1.2.2",
    "rimraf": "^5.0.5",
    "rollup": "^4.12.0",
    "typescript": "^5.3.3"
  }
}
